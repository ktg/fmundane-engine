                    }
                ],
                "data": [
                    "next"
                ]
            },
            {
                "type": "button",
                "name": "done",
                "id": "done",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[lenovo]:9102/api/message",
                                "params": {
                                    "query": {
                                        "message": "I am here!"
                                    }
                                },
                                "method": "GET",
