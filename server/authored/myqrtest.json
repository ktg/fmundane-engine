[
    {
        "id": "qrtest",
        "start": {
            "actions": [
                []
            ],
            "event": "starter"
        },
        "events": [
            {
                "type": "button",
                "name": "starter",
                "id": "starter",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[lenovo]:9102/api/qrcode",
