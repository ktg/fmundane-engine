[
    {
        "id": "localtest",
        "start": {
            "actions": [
                []
            ],
            "event": "start"
        },
        "events": [
            {
                "type": "button",
                "name": "start",
                "id": "start",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[lenovo]:9102/api/media/play",
                                "params": {
                                    "query": {
                                        "media": "TV.mp4"
                                    }
                                },
                                "method": "GET",
                                "delay": 0
                            }
                        ]
                    ]
                },
                "rules": [
                    {
                        "rule": {
                            "operator": "equals",
                            "operand": "next"
                        },
                        "next": "imagetest",
                        "actions": []
                    }
                ],
                "data": [
                    "next"
                ]
            },
            {
                "type": "button",
                "name": "imagetest",
                "id": "imagetest",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[lenovo]:9102/api/image",
                                "params": {
                                    "query": {
                                        "image": "phone.jpg"
                                    }
                                },
                                "method": "GET",
                                "delay": 0
                            }
                        ]
                    ]
                },
                "rules": [
                    {
                        "rule": {
                            "operator": "equals",
                            "operand": "more"
                        },
                        "next": "moremedia",
                        "actions": []
                    }
                ],
                "data": [
                    "more"
                ]
            },
            {
                "type": "button",
                "name": "moremedia",
                "id": "moremedia",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[lenovo]:9102/api/media/play",
                                "params": {
                                    "query": {
                                        "media": "countdown.mp4"
                                    }
                                },
                                "method": "GET",
                                "delay": 0
                            }
                        ]
                    ]
                },
                "rules": [],
                "data": []
            }
        ]
    }
]